{
  "name": "@serh/vue-soundmanager",
  "version": "1.0.2",
  "description": "Sound component to play audio in your web apps. Backed by the mighty soundmanager2 library.",
  "main": "dist/vue-soundmanager.umd.js",
  "module": "dist/vue-soundmanager.esm.js",
  "unpkg": "dist/vue-soundmanager.min.js",
  "browser": {
    "./sfc": "src/vue-soundmanager.vue"
  },
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/my-component.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/my-component.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/my-component.min.js"
  },
  "dependencies": {
    "soundmanager2": "^2.97.20170602"
  },
  "devDependencies": {
    "rollup": "^1.17.0",
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^11.1.0",
    "rollup-plugin-vue": "^5.0.1",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/serhn/vue-soundmanager.git"
  },
  "keywords": [
    "vue",
    "vue-compoment",
    "sound",
    "audio",
    "player",
    "mp3",
    "mpeg4",
    "html5",
    "soundmanager2"
  ],
  "author": "Serhii Nikolaichuk",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/serhn/vue-soundmanager/issues"
  },
  "homepage": "https://github.com/serhn/vue-soundmanager#readme"
}
